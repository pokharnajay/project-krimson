# Flask
FLASK_ENV=development
SECRET_KEY=delulu

# JWT
JWT_SECRET_KEY=delulu
JWT_ACCESS_TOKEN_EXPIRES=3600
JWT_REFRESH_TOKEN_EXPIRES=2592000

# Admin API Key (never expires)
ADMIN_API_KEY=delulu

# OpenRouter
# Get your API key from https://openrouter.ai/
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_MODEL=openai/gpt-4-turbo

# Pinecone
# Get your API key from https://www.pinecone.io/
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=us-east-1
PINECONE_INDEX_NAME=youtube-transcripts

# Processing
MAX_THREADS=5
VECTOR_SEARCH_TYPE=similarity
TOP_K_RESULTS=5


# SUPABASE
# Get your credentials from https://supabase.com/
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_SERVICE_KEY=your_supabase_service_key_here

# Rate Limiting
RATELIMIT_ENABLED=True
RATELIMIT_STORAGE_URL=memory://
RATELIMIT_DEFAULT=100/hour

# CORS
CORS_ORIGINS=*

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/logging.txt